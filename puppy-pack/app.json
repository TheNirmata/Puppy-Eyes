{
  "expo": {
    "name": "puppy-pack",
    "slug": "puppy-pack",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      ["expo-font",
        {
          "fonts":["app/(login)/assets/fonts/PixelCode.ttf"]
        }],
      "expo-splash-screen"
    ],
    "scheme": "puppy-pack-scheme",
    "main": "index.js"
  }
}
